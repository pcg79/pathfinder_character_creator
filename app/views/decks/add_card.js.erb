$('div#deck-cards .list-group').append('<%= render partial: "card", object: @card, locals: { path: remove_card_deck_path(@deck), action: "remove" } %>');
$('div#deck-cards .list-group p').remove();

var $count_td = $('div#current-counts tr#<%= status_type(@card.type) %> td.count');
var count = parseInt($count_td.html());

$count_td.html(count + 1);

$('div#current-counts tr#<%= status_type(@card.type) %>').removeClass().addClass('<%= card_type_status_class(@deck, @card.type) %>');

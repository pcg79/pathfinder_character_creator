$('div#deck-cards .list-group').append('<%= link_to display_data(@card), remove_card_deck_path(@deck), class: "list-group-item", id: "card-#{@card.id}", data: { "card_id" => @card.id, "action" => "remove" } %>');

$('div#deck-cards .list-group p').remove();

var $count_td = $('div#current-counts tr#<%= @card_type %> td.count');
var count = parseInt($count_td.html());

$count_td.html(count + 1);